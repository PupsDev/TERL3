<!DOCTYPE html>
{% trans_default_domain 'global' %}
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {# Bootstrap#}
        <link rel="stylesheet" href="/assets/styles/bootstrap.min.css">
        <link rel="stylesheet" href="/assets/styles/global.css">
        {% block stylesheets %}
            {#{{ encore_entry_link_tags('app') }}#}
        {% endblock %}
    </head>
    <body>
        <a href="{{ path('app_home') }}" id="home_link" >{{ 'global.home' | trans }}</a>
        <div class="center m-2">

                {% block body %}{% endblock %}

        </div>
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
        {# Bootstrap
        <script src="/assets/scripts/bootstrap/bootstrap.min.js"></script>
        <script src="/assets/scripts/bootstrap/bootstrap.bundle.min.js"></script>
        <script src="/assets/scripts/bootstrap/bootstrap.esm.min.js"></script>#}
        {% block script %}
            {#{{ encore_entry_script_tags('app') }}#}
        {% endblock %}
    </body>

</html>
